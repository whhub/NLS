<Window x:Class="SampleApplication.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="SampleDictionary.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <Label Content="{DynamicResource UID_Sample_PatientName_Label}" Height="28" HorizontalAlignment="Left" Margin="132,145,0,0" Name="label1" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="270,145,0,0" Name="textBox1" VerticalAlignment="Top" Width="120" />
        <Label Content="{DynamicResource UID_Sample_PatientID_Label}" Height="28" HorizontalAlignment="Left" Margin="148,96,0,0" Name="label2" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="270,101,0,0" Name="textBox2" VerticalAlignment="Top" Width="120" />
    </Grid>
</Window>
